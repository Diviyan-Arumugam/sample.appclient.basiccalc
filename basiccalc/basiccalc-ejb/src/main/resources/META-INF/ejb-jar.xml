<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/ejb-jar_3_2.xsd"
         version="3.2">
         <enterprise-beans>
             <session id="Session_1">
                <description>Basic Calculator to add, subtract, multiply, divide</description>
                <display-name>Basic Calculator Stateless Session</display-name>
                <ejb-name>BasicCalculator</ejb-name>
                <remote>net.wasdev.sample.basiccalc.ejb.BasicCalculatorSessionBeanRemote</remote>
                <ejb-class>net.wasdev.sample.basiccalc.ejb.BasicCalculatorSessionBean</ejb-class>
                <session-type>Stateless</session-type>
                <transaction-type>Container</transaction-type>
             </session>
          </enterprise-beans>
</ejb-jar>
